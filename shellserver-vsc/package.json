{
	"name": "shellserver-vsc",
	"displayName": "ShellServer-vsc",
	"description": "send python cmd to shellServer",
	"publisher": "OlivierArgentieri",
	"version": "0.0.5",
	"repository": "https://github.com/OlivierArgentieri/shellServer_vsc_extension",
	"engines": {
		"vscode": "^1.22.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:shellserver-vsc.setport",
		"onCommand:shellserver-vsc.sendto"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "shellserver-vsc.setport",
				"title": "Set Port"
			},
			{
				"command": "shellserver-vsc.sendto",
				"title": "Send To"
			}
		],
		"keybindings": [
			{
				"command": "shellserver-vsc.sendto",
				"key": "ctrl+e ctrl+e"
			},
			{
				"command": "shellserver-vsc.setport",
				"key": "shift+ctrl+e"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.22.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.2.2",
		"@types/node": "14.x",
		"eslint": "^7.27.0",
		"@typescript-eslint/eslint-plugin": "^4.26.0",
		"@typescript-eslint/parser": "^4.26.0",
		"glob": "^7.1.7",
		"mocha": "^10.1.0",
		"typescript": "^4.3.2",
		"vscode-test": "^1.5.2"
	},
	"dependencies": {
		"net": "^1.0.2"
	}
}
